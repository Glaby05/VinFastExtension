    {
      "manifest_version": 3,
      "name": "VinFast Extension",
      "version": "1.0",
      "description": "A simple example Chrome extension.",
      "permissions": ["activeTab", "scripting", "storage"],
      "host_permissions": ["*://*vinfastauto.ca/*"], 
      "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
      },
      "content_scripts": [
            {
            "matches": ["*://*.vinfastauto.ca/*"],
            "js": ["content.js"]
            }
        ]
    }